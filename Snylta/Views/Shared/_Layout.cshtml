<!DOCTYPE html>
<html>
<head>
    <link href="https://fonts.googleapis.com/css?family=Noto+Sans+TC" rel="stylesheet">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Snylta</title>
    @*<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">*@

    @*<link href="~/css/bootstrap.min.css" rel="stylesheet" />*@
    <link href="~/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
    @*<link href="~/css/material-kit.css" rel="stylesheet" />*@
    
    <style>
        Body {
            font-family: 'Noto Sans TC', sans-serif;
        }
    </style>
</head>
<body>
    <nav class="navbar navbar-expand-lg fixed-top bg-secondary">
        @*navbar-light bg-light*@

        @*<nav class="navbar-expand-lg navbar-light bg-light">*@
        <div class="container">
            <a class="navbar-brand" asp-controller="Home" asp-action="Index"><strong>Snylta!</strong></a>
            @*<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"><i></i></span>
            </button>*@
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <ul class="navbar-nav mr-auto">
                    @*<li class="nav-item active">
            <a class="nav-link" asp-controller="Home" asp-action="Index">Home <span class="sr-only">(current)</span></a>
        </li>*@
                    @*<li class="nav-item">
            <a class="nav-link" asp-controller="Home" asp-action="About">About</a>
        </li>*@
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Prylar
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">

                            <a class="dropdown-item" asp-controller="Things" asp-action="Create">Lägg till pryl</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" asp-controller="Things" asp-action="Index">Andras prylar (snylta!)</a>
                            <a class="dropdown-item" asp-controller="Things" asp-action="MyThings">Mina prylar</a>
                            <a class="dropdown-item" asp-controller="Things" asp-action="Snyltningar">Mina pågående snyltningar</a>
                        </div>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Grupper
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">

                            <a class="dropdown-item" asp-controller="Groups" asp-action="Create">Lägg till grupp</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" asp-controller="Groups" asp-action="Index">Mina grupper</a>

                        </div>
                    </li>
                    @*<li class="nav-item">
                        <a class="nav-link" asp-controller="Groups" asp-action="Index">Grupper</a>
                    </li>*@
                </ul>
                <div class="nav navbar-nav navbar-right">
                    <partial name="_LoginPartial" />
                </div>
            </div>

        </div>
    </nav>


    @*<partial name="_CookieConsentPartial" />*@

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2019 - Snylta</p>
        </footer>
    </div>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
